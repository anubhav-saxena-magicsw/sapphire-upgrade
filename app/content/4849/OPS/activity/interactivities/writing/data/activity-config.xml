				<data source="interactivities/writing/activityScreen/screen.json"></data>
				<style source="interactivities/writing/activityScreen/screen.css"></style>
		 </activity>
		</activities>
	</region>
	

</regions>
<playerSettings>
    <release>false</release>
    <showPreloader>false</showPreloader>
    <activityPath>
