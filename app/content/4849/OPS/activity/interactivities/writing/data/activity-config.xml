<?xml version="1.0" encoding="utf-8"?>
<player>
<regions>

	
	<region id="mActivity" type="mActivity" styleClass="container-fluid contentblock" loadPriority="1">
		<activities defaultLaunchID="1">
			<activity id="1" name="activityScreen" modelPath="" modelName="" groupName="" lang="sample" class="sim-pictor">
