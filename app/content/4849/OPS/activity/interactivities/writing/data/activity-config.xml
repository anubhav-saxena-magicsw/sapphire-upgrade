      <support>false</support>
      <rootPath>data/local</rootPath>
      <default>en</default>
    </language>
    <size>
      <align>
        <horizontal>center</horizontal>
        <vertical>top</vertical>
      </align>
      <scale maxScale="" minScale="" isScalable="false"/>
      <width></width>
      <height></height>
    </size>
    <webService></webService>
