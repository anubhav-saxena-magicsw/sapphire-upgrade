      <showTacer>true</showTacer>
      <errorLogger>true</errorLogger>
    </debug>
    <themes defaultTheme="1" prefix="theme">
      <theme id="1" prefix="theme">
        <label>1</label>
        <path>shell/themes/theme1/theme1.css</path>
      </theme>
    </themes>
